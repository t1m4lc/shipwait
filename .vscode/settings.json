{
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.removeUnusedImports": "explicit"
  },
  "files.autoSave": "afterDelay",
  "files.exclude": {
    ".output": true,
    ".nuxt": true,
    ".data": true,
    "node_modules": true
  },
  "search.exclude": {
    ".output": true,
    ".nuxt": true,
    ".data": true,
    "node_modules": true
  }
}

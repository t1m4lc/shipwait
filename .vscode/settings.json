{
  "deno.enablePaths": [
    "supabase/functions"
  ],
  "deno.lint": true,
  "deno.unstable": [
    "bare-node-builtins",
    "byonm",
    "sloppy-imports",
    "unsafe-proto",
    "webgpu",
    "broadcast-channel",
    "worker-options",
    "cron",
    "kv",
    "ffi",
    "fs",
    "http",
    "net"
  ],
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnSave": true,
  "files.autoSave": "afterDelay",
  "files.exclude": {
    ".output": true,
    ".nuxt": true,
    ".data": true,
    "node_modules": true
  },
  "search.exclude": {
    ".output": true,
    ".nuxt": true,
    ".data": true,
    "node_modules": true
  }
}
